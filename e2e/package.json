{
    "name": "pagy.e2e",
    "description": "Pagy E2E Testing",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=20.9.0"
    },
    "devDependencies": {
        "@cypress/snapshot": "^2.1.7",
        "@types/node": "20.12.7",
        "@typescript-eslint/eslint-plugin": "7.7.0",
        "@typescript-eslint/parser": "7.7.0",
        "cypress": "13.8.0",
        "cypress-html-validate": "6.1.0",
        "eslint": "8.57.0",
        "eslint-plugin-align-assignments": "^1.1.2",
        "eslint-plugin-cypress": "2.15.2",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "6.1.1",
        "html-validate": "8.18.1",
        "start-server-and-test": "2.0.3",
        "typescript": "5.4.5"
    },
    "scripts": {
        "lint-fix": "eslint --fix cypress --ext .ts",
        "test": "start-test \"bundle exec pagy $APP -p 8080\" 8080 \"cypress run --spec cypress/e2e/$APP/**/*\"",
        "test-open": "start-test \"bundle exec pagy $APP -p 8080\" 8080 \"cypress open\""
    }
}
